<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.isee_v1.MainActivity"
    tools:ignore="MergeRootFrame" >
<ImageView
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType = "fitXY"
        android:src="@drawable/abc_ab_bottom_solid_light_holo" />
    
    <ImageView
        android:id="@+id/Search_Buttom"
        android:layout_width="350px"
        android:layout_height="350px"
        android:layout_marginLeft="-185px"
        android:layout_marginTop="-185px"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center|center_horizontal"
        android:onClick="Search_Buttom_Onclick"
        android:src="@drawable/search" />
    <ImageView
        android:id="@+id/Photo_Buttom"
        android:layout_width="350px"
        android:layout_height="350px"
        android:layout_marginLeft="185px"
        android:layout_marginTop="-185px"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center|center_horizontal"
        android:onClick="Photo_Buttom_Onclick"
        android:src="@drawable/photo" />
    <ImageView
        android:id="@+id/Draw_Buttom"
        android:layout_width="350px"
        android:layout_height="350px"
        android:layout_marginLeft="-185px"
        android:layout_marginTop="185px"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center|center_horizontal"
        android:onClick="Draw_Buttom_Onclick"
        android:src="@drawable/draw" />
    <ImageView
        android:id="@+id/Upload_Buttom"
        android:layout_width="350px"
        android:layout_height="350px"
        android:layout_marginLeft="185px"
        android:layout_marginTop="185px"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center|center_horizontal"
        android:onClick="Upload_Buttom_Onclick"
        android:src="@drawable/upload" />

    

</FrameLayout>

